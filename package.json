{
	"name": "@dependency/javascriptTestRunner",
	"version": "1.1.6",
	"main": "./entrypoint/programmaticAPI",
	"description": "Mocha test runner for unit tests and integration tests.",
	"bin": {
		"mochaCli": "./entrypoint/cli/index.js"
	},
	"scripts": {
		"build": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".buildSourceCode.moduleProject({}, {compile:[/**'./test'*/]})\"",
		"buildAndRelease": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".buildAndRelease.moduleProject({})\""
	},
	"dependencies": {
		"@dependency/javascriptTranspilation": "https://github.com/AppScriptIO/javascriptTranspilation#semver:^1.x.x",
		"@dependency/nodejsLiveReload": "https://github.com/AppScriptIO/nodejsLiveReload#semver:^1.x.x",
		"@dependency/parseKeyValuePairSeparatedBySymbol": "https://github.com/AppScriptIO/parseKeyValuePairSeparatedBySymbol#semver:^1.x.x",
		"chai": "^4.1.2",
		"mocha": "*",
		"moment": "*",
		"should": "*",
		"sinon": "*"
	},
	"devDependencies": {
		"@dependency/deploymentScript": "https://github.com/AppScriptIO/deploymentScript#semver:^1.x.x",
		"@dependency/javascriptStaticAnalysis": "https://github.com/AppScriptIO/javascriptStaticAnalysis#semver:^1.x.x",
		"@dependency/scriptManager": "https://github.com/AppScriptIO/scriptManager#semver:^1.x.x",
		"typescript": "^3.3.3333"
	}
}